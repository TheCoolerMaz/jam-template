<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.10.0//EN" "http://www.gwtproject.org/doctype/2.10.0/gwt-module.dtd">
<module rename-to="html">
    <inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />
    <inherits name='com.badlogic.ashley_gwt' />
    <inherits name='JamTemplate' />

    <replace-with class="com.jamtemplate.graphics.GwtShaderPipelineFactory">
        <when-type-is class="com.jamtemplate.graphics.IShaderPipelineFactory" />
    </replace-with>
    <source path="" />
    <entry-point class='com.jamtemplate.gwt.GwtLauncher' />
    <set-configuration-property name="gdx.assetpath" value="../assets" />
    <set-configuration-property name="xsiframe.failIfScriptTag" value="FALSE"/>
    <set-configuration-property name='gwt.permutation.number' value='0' />
</module>
